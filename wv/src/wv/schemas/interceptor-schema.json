{
    "$schema": "http://json-schema.org/schema",
    "id": "WVInterceptorSchematic",
    "title": "ng new wv-Interceptor options schema",
    "type": "object",
    "description": "Create a new interceptor",
    "properties": {
        "name": {
            "type": "string",
            "description": "The name of the interceptor",
            "x-prompt": "Name:",
            "$default": {
                "$source": "argv",
                "index": 0
            }
        },
        "path": {
            "type": "string",
            "description": "The path of the enum to be created",
            "$default": {
                "$source": "argv",
                "index": 1
            }
        },
        "project": {
            "type": "string",
            "description": "The project of enum module.",
            "$default": {
              "$source": "projectName"
            }
        } 
    }
}